SOUND_IS_PLAYING:   equ 0x9FE9
SOUND_STOP:         equ 0x9FF1
SOUND_STOP_ALL:     equ 0x9FF3
SOUND_AY_REG_W:     equ 0xA00A
SOUND_AY_LOOP:      equ 0xA01B
SOUND_AY_DONE:      equ 0xA01E
SOUND_DATA_PTR:     equ 0xA031
